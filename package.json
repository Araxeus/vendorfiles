{
  "name": "vendorfiles",
  "packageManager": "yarn@3.5.0",
  "type": "module",
  "version": "1.0.0-alpha.1",
  "scripts": {
    "start": "tsc && node ./dist/cli.js",
    "test": "tsc && node ./dist/test.js",
    "build": "tsc",
    "check": "tsc --noEmit && rome ci ."
  },
  "bin": {
    "vendor": "./dist/cli.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js"
    }
  },
  "dependencies": {
    "@commander-js/extra-typings": "^10.0.3",
    "@octokit/rest": "^19.0.7",
    "ci-info": "^3.8.0",
    "commander": "^10.0.0",
    "dotenv": "^16.0.3",
    "parse-json": "^6.0.2",
    "write-pkg": "^5.1.0"
  },
  "devDependencies": {
    "@types/node": "^18.15.4",
    "@types/parse-json": "^4.0.0",
    "read-pkg-up": "^9.1.0",
    "rome": "^11.0.0",
    "typescript": "^5.0.2"
  },
  "vendorDependencies": {
    "Cooltipz.css": {
      "name": "Cooltipz.css",
      "version": "v2.2.1",
      "repository": "https://github.com/jackdomleo7/Cooltipz.css",
      "files": [
        "cooltipz.min.css",
        {
          "LICENSE": "COOLTIPZ_LICENSE"
        }
      ],
      "vendorFolder": "{vendorFolder}"
    },
    "Coloris": {
      "name": "Coloris",
      "version": "v0.18.0",
      "repository": "https://github.com/mdbassit/Coloris",
      "files": [
        "dist/coloris.min.js",
        "dist/coloris.min.css",
        {
          "LICENSE": "COLORIS_LICENSE"
        }
      ],
      "vendorFolder": "{vendorFolder}"
    }
  },
  "vendorConfig": {
    "vendorFolder": "./vendor"
  }
}
